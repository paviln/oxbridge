{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended", 
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": "off"
    }
}
